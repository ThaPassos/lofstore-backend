{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/criar-pagamento",
      "dest": "/api/criar-pagamento.js"
    },
    {
      "src": "/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/status/(.*)",
      "dest": "/api/status.js"
    }
  ]
}